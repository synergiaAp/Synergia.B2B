@Scripts.Render("~/bundles/activitiesAnalysis/activitiesAnalysisList")

@{
    ViewBag.Title = "Analiza aktywności";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header card-header-text" data-background-color="blueSynergia">
                <h4 class="card-title">Lista aktywności użytkowników</h4>
            </div>
            <div class="card-content">
                <div class="toolbar">
                </div>
                <div class="col-md-8"></div>
                <div class="col-md-2">
                    <div class="form-group label-floating">
                        @Html.Label("Od początku", new { @class = "control-label datepickerLabel required", @for= "StartDateFilter" })
                        @Html.TextBox("StartDateFilter", "", new { @class = "form-control monthpicker" })
                    </div>
                </div>
                <div class="col-md-2">
                    <div class="form-group label-floating">
                        @Html.Label("Do końca", new { @class = "control-label datepickerLabel required", @for= "EndDateFilter" })
                        @Html.TextBox("EndDateFilter", "", new { @class = "form-control monthpicker" })
                    </div>
                </div>
                <div class="material-datatables">
                    <table id="acitivitiesAnalysisTable" class="table table-striped table-no-bordered table-hover dataTable" cellspacing="0" style="width:100%; display:none">
                        <thead>
                            <tr>
                                <th>Użytkownik</th>
                                <th>Wizyty</th>
                                <th>Dobory projektów</th>
                                <th>Umowy handlowe</th>
                                <th>Oferty cenowe</th>
                                <th>Wartość planowanych zamówień</th>
                                <th>Wartość zamówień</th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="card card-chart">
            <div class="card-header" data-background-color="rose" data-header-animation="false">
                <canvas class="ct-chart" id="meetingsChart"></canvas>
            </div>
            <div class="card-content">
                <div class="card-actions">
                    <button type="button" class="btn btn-danger btn-simple fix-broken-card">
                        <i class="material-icons">build</i> Fix Header!
                    </button>
                    <button type="button" class="btn btn-info btn-simple" rel="tooltip" data-placement="bottom" title="Refresh">
                        <i class="material-icons">refresh</i>
                    </button>
                    <button type="button" class="btn btn-default btn-simple" rel="tooltip" data-placement="bottom" title="Change Date">
                        <i class="material-icons">edit</i>
                    </button>
                </div>
                <h4 class="card-title">Liczba spotkań handlowców</h4>
                <p class="category"> w wybranym okresie</p>
            </div>
            @*<div class="card-footer">
                <div class="stats">
                    <i class="material-icons">access_time</i> campaign sent 2 days ago
                </div>
            </div>*@
        </div>
    </div>
